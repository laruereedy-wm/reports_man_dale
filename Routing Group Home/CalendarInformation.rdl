      <Query>
        <DataSourceName>VM250_WIPS_Production</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Date">
            <Value>=Parameters!Date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     CalendarDate, ShipDate, ShipGroup, ProdDay, PromptDay, OfficeDay, ProdSequenceNum, PromptSequenceNum, OfficeSequenceNum
