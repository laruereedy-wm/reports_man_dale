  <DataSets>
    <DataSet Name="OrderComment">
      <Query>
        <DataSourceName>VM250_OrderEntry</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrderNum">
            <Value>=Parameters!OrderNum.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ItemNum">
            <Value>=Parameters!ItemNum.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT OrderNum, ItemNum,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%1')) AS COM1,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%2')) AS COM2,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%3')) AS COM3,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%4')) AS COM4,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%5')) AS COM5,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%6')) AS COM6,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%7')) AS COM7,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%8')) AS COM8,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%9')) AS COM9,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) LIKE '%0')) AS COM10,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '11')) AS COM11,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '12')) AS COM12,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '13')) AS COM13,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '14')) AS COM14,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '15')) AS COM15,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '16')) AS COM16,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '17')) AS COM17,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '18')) AS COM18,
                          (SELECT     Comment
                            FROM          OrderItemComments AS OrderItemComments_1
                            WHERE      (OrderNum = @OrderNum) AND (ItemNum = @ItemNum) AND (RIGHT(RelativeRecordNum, 2) = '19')) AS COM19
FROM         OrderItemComments AS OrderItemComments_1
WHERE     (OrderNum = @OrderNum) AND (ItemNum = @ItemNum)</CommandText>
