  <DataSets>
    <DataSet Name="ItemOptions">
      <Query>
        <DataSourceName>VM250_OrderEntryOrderInfo</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prmOrderNum">
            <Value>=Parameters!prmOrderNum.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@prmItemNum">
            <Value>=Parameters!prmItemNum.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     OrderOptions.RelativeRecordNum, OrderOptions.OrderNum, OrderOptions.ItemNum, OrderOptions.OptionID, OrderOptions.OptionAccessCode, 
                      OrderOptions.OptionDesc, OrderOptions.OptionQty, OrderOptions.OptionPrice, OrderOptions.OptionCubicVolAdj, OrderOptions.OptionPrintPriceFlag, 
                      OrderOptions.OptionWidth, OrderOptions.OptionHeight, OrderOptions.OptionDepth, OrderOptionComments.Comment
FROM         OrderOptions LEFT OUTER JOIN
                      OrderOptionComments ON OrderOptions.OrderNum = OrderOptionComments.OrderNum AND OrderOptions.ItemNum = OrderOptionComments.ItemNum AND 
                      OrderOptions.OptionAccessCode = OrderOptionComments.OptionAccessCode
WHERE     (OrderOptions.OrderNum = @prmOrderNum) AND (OrderOptions.ItemNum = @prmItemNum)</CommandText>
      </Query>
