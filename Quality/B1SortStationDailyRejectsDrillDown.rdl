    <DataSet Name="RepairQue">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <CommandText>SELECT     CSSI_TRACKING_LASTSCAN.TRACKINGPOINT, COUNT(CSSI_TRACKING_LASTSCAN.TRACKINGPOINT) AS QUEUESIZE, SUM(MOS.MOS_QTY) AS TotalParts
FROM         CSSI_TRACKING_LASTSCAN INNER JOIN
                      MOS ON CSSI_TRACKING_LASTSCAN.MOS_TYPE = MOS.MOS_TYPE COLLATE SQL_Latin1_General_CP1_CI_AS AND 
                      CSSI_TRACKING_LASTSCAN.MOS_JOB = MOS.MOS_JOB COLLATE SQL_Latin1_General_CP1_CI_AS AND 
                      CSSI_TRACKING_LASTSCAN.MOS_LOT = MOS.MOS_LOT COLLATE SQL_Latin1_General_CP1_CI_AS
WHERE     (CSSI_TRACKING_LASTSCAN.TRACKINGPOINT = N'065R')
GROUP BY CSSI_TRACKING_LASTSCAN.TRACKINGPOINT, MOS.MOS_STATUS
HAVING      (MOS.MOS_STATUS &lt; 3)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TRACKINGPOINT">
          <DataField>TRACKINGPOINT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QUEUESIZE">
          <DataField>QUEUESIZE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalParts">
          <DataField>TotalParts</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
