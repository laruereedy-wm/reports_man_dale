    <DataSet Name="AllReworks">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     CSSI_REWORKS.REWORKID, CSSI_REWORKS.USERID, CSSI_REWORKS.REWORK_WCKEY, CONVERT(int, CSSI_REWORKS.REWORK_QTY) 
                      AS REWORK_QTY, CSSI_REWORKS.REASON_CODE, CSSI_REWORKS.REWORK_MOSLOT, CSSI_REWORKS.REWORK_MOSUSER1, 
                      CSSI_REWORKS.REWORK_TIMESTAMP, CSSI_DEV_CODE.DESCRIPTION, 1 AS TOTAL, CSSI_REWORKS.REWORK_MOSJOB, 
                      CSSI_REWORKS.REWORK_MOSJOB AS Expr1, LEFT(MOS.MOS_REV, 1) AS DESIGNGROUP, MOS.MOS_IMKEY, MOS.MOS_WANTDATE
FROM         MOS RIGHT OUTER JOIN
                      CSSI_REWORKS ON MOS.MOS_JOB = CSSI_REWORKS.REWORK_MOSJOB COLLATE Latin1_General_BIN LEFT OUTER JOIN
                      CSSI_DEV_CODE ON CSSI_REWORKS.REASON_CODE = CSSI_DEV_CODE.DEV_CODE
WHERE     (CSSI_REWORKS.REWORK_TIMESTAMP &gt;= @startDate) AND (CSSI_REWORKS.REWORK_TIMESTAMP &lt;= @endDate) AND 
                      (CSSI_REWORKS.REWORK_WCKEY = N'C1107' OR
                      CSSI_REWORKS.REWORK_WCKEY = N'EC1107' OR
                      CSSI_REWORKS.REWORK_WCKEY = N'JC1107' OR
                      CSSI_REWORKS.REWORK_WCKEY = N'RC1107')
ORDER BY CSSI_DEV_CODE.DESCRIPTION, CSSI_REWORKS.REWORK_MOSJOB</CommandText>
      </Query>
