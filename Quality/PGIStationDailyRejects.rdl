    <DataSet Name="BanderInfo">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT 
                      CSSI_REWORKS.REWORKID, CSSI_REWORKS.USERID, CSSI_REWORKS.REWORK_WCKEY, CONVERT(int, CSSI_REWORKS.REWORK_QTY) 
                      AS REWORK_QTY, CSSI_REWORKS.REASON_CODE, CSSI_DEV_CODE.DESCRIPTION, CSSI_REWORKS.REWORK_MOSLOT, 
                      CSSI_REWORKS.REWORK_MOSUSER1, CSSI_REWORKS.REWORK_TIMESTAMP, MAXRESULTS.MAXDATE AS LX_DATE, 
                      CSSI_REWORKS.REWORK_MOSJOB AS LX_MOSJOB
FROM         (SELECT     MAX(LX_DATE) AS MAXDATE, LX_MOSJOB
                       FROM          LX AS t1
                       WHERE      (LX_WCKEY LIKE '%053%')
                       GROUP BY LX_MOSJOB, LX_WCKEY) AS MAXRESULTS INNER JOIN
                      CSSI_REWORKS ON MAXRESULTS.LX_MOSJOB = CSSI_REWORKS.REWORK_MOSJOB COLLATE Latin1_General_BIN LEFT OUTER JOIN
                      CSSI_DEV_CODE ON CSSI_REWORKS.REASON_CODE = CSSI_DEV_CODE.DEV_CODE
WHERE     (CSSI_REWORKS.REWORK_TIMESTAMP &gt;= @startDate) AND (CSSI_REWORKS.REWORK_TIMESTAMP &lt;= @endDate)
ORDER BY CSSI_DEV_CODE.DESCRIPTION</CommandText>
      </Query>
      <Fields>
        <Field Name="REWORKID">
          <DataField>REWORKID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="USERID">
          <DataField>USERID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REWORK_WCKEY">
          <DataField>REWORK_WCKEY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REWORK_QTY">
          <DataField>REWORK_QTY</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="REASON_CODE">
          <DataField>REASON_CODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIPTION">
          <DataField>DESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REWORK_MOSLOT">
          <DataField>REWORK_MOSLOT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REWORK_MOSUSER1">
          <DataField>REWORK_MOSUSER1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="REWORK_TIMESTAMP">
          <DataField>REWORK_TIMESTAMP</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LX_DATE">
          <DataField>LX_DATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LX_MOSJOB">
          <DataField>LX_MOSJOB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LX_DATE_ONLY">
          <Value>=DateValue(Fields!LX_DATE.Value)</Value>
        </Field>
        <Field Name="WRONG_SIZE">
          <Value>=IIf(Fields!REASON_CODE.Value = "1", True, False)</Value>
        </Field>
        <Field Name="OUT_OF_SQUARE">
          <Value>=IIf(Fields!REASON_CODE.Value = "2", True, False)</Value>
        </Field>
        <Field Name="BANDED_WRONG">
          <Value>=IIf(Fields!REASON_CODE.Value = "40", True, False)</Value>
        </Field>
        <Field Name="LOOSE_BANDING">
          <Value>=IIf(Fields!REASON_CODE.Value = "23", True, False)</Value>
        </Field>
        <Field Name="IS_FIRST_SHIFT">
          <Value>=IIf(TimeValue(Fields!LX_DATE.Value) &gt;= "6:30 AM" And TimeValue(Fields!LX_DATE.Value) &lt;= "7:30 PM", True, False)</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TotalPieces">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     LX.LX_WCKEY, LX.LX_SEQNUM, LX.LX_EMKEY, LX.LX_MOSJOB, LX.LX_QTY, MOS.MOS_IMKEY, IM.IM_DESCR, IM.IM_KEY, IM.IM_UNIT_S, 
                      LX.LX_DATETIME
FROM         LX LEFT OUTER JOIN
                      MOS ON LX.LX_MOSTYPE = MOS.MOS_TYPE AND LX.LX_MOSJOB = MOS.MOS_JOB AND LX.LX_MOSLOT = MOS.MOS_LOT AND 
                      LX.LX_MOSSEQNUM = MOS.MOS_SEQNUM1 LEFT OUTER JOIN
                      IM ON MOS.MOS_IMKEY = IM.IM_KEY
WHERE     (LX.LX_DATETIME &gt;= @startDate) AND (LX.LX_DATETIME &lt;= @endDate) AND (LX.LX_WCKEY = 'C1107' OR
                      LX.LX_WCKEY = 'EC1107' OR
                      LX.LX_WCKEY = 'JC1107' OR
                      LX.LX_WCKEY = 'RC1107')
ORDER BY LX.LX_WCKEY, LX.LX_EMKEY, LX.LX_DATETIME</CommandText>
      </Query>
