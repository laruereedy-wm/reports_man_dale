        </Field>
        <Field Name="MOS_IMKEY">
          <DataField>MOS_IMKEY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OP_WCKEY">
          <DataField>OP_WCKEY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RNO_LINE_NO">
          <DataField>RNO_LINE_NO</DataField>
