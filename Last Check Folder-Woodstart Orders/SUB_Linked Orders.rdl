  <DataSources>
    <DataSource Name="PKI">
      <DataSourceReference>PKI</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>5038857e-d2ba-4739-b6f1-2aa8c4bb6e2c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@JOB_LOT">
            <Value>=Parameters!JOB_LOT.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     MOS.MOS_JOB, MOS.MOS_LOT, RQ.RQ_IMKEY, PR.PR_MOSJOB, MOS_1.MOS_IMKEY, OP.OP_WCKEY, RNO.RNO_LINE_NO, 
                      RNO.RNO_DESCR
FROM         MOS INNER JOIN
                      RQ ON MOS.MOS_TYPE = RQ.RQ_MOSTYPE AND MOS.MOS_JOB = RQ.RQ_MOSJOB AND MOS.MOS_LOT = RQ.RQ_MOSLOT AND 
                      MOS.MOS_SEQNUM1 = RQ.RQ_MOSSEQNUM INNER JOIN
                      PR ON RQ.RQ_SEQNUM = PR.PR_RQSEQNUM INNER JOIN
                      MOS AS MOS_1 ON PR.PR_MOSTYPE = MOS_1.MOS_TYPE AND PR.PR_MOSJOB = MOS_1.MOS_JOB AND PR.PR_MOSLOT = MOS_1.MOS_LOT AND 
                      PR.PR_MOSSEQNUM = MOS_1.MOS_SEQNUM1 INNER JOIN
                      OP ON MOS_1.MOS_TYPE = OP.OP_MOSTYPE AND MOS_1.MOS_JOB = OP.OP_MOSJOB AND MOS_1.MOS_LOT = OP.OP_MOSLOT AND 
                      MOS_1.MOS_SEQNUM1 = OP.OP_MOSSEQNUM INNER JOIN
                      RNO ON OP.OP_SEQNUM = RNO.RNO_OPSEQNUM
WHERE     (OP.OP_WCKEY = 'NOTE') AND (RNO.RNO_LINE_NO = 2) AND (MOS.MOS_JOB = @JOB_LOT)</CommandText>
      </Query>
      <Fields>
        <Field Name="MOS_JOB">
          <DataField>MOS_JOB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MOS_LOT">
