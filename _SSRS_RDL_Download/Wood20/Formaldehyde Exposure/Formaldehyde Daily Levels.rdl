  <DataSets>
    <DataSet Name="mainData">
      <Fields>
        <Field Name="SCANDATE">
          <DataField>SCANDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EMPLOYEENUMBER">
          <DataField>EMPLOYEENUMBER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LOCATION">
          <DataField>LOCATION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HOURS_EXPOSED">
          <DataField>HOURS_EXPOSED</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="FORMALDEHYDE_LEVEL">
          <DataField>FORMALDEHYDE_LEVEL</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="TOTAL_EXPOSURE_PPM">
          <Value>=Fields!HOURS_EXPOSED.Value * Fields!FORMALDEHYDE_LEVEL.Value</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PK1</DataSourceName>
        <CommandText>DECLARE @SQL as varchar(1000)

IF @location = 'All Locations' 
SET @SQL = 'SELECT     SCANDATE, EMPLOYEENUMBER, LOCATION, HOURS_EXPOSED, FORMALDEHYDE_LEVEL
FROM         CSSI_FORMALDEHYDE_EXPOSURE
WHERE     CSSI_FORMALDEHYDE_EXPOSURE.SCANDATE &gt;= ' + '''' + convert(varchar(50), @startDate) + '''' + ' AND CSSI_FORMALDEHYDE_EXPOSURE.SCANDATE &lt;= dateadd(hh, 24, ''' + convert(varchar(50), @endDate) +''')'


ELSE 

SET @SQL = 'SELECT     SCANDATE, EMPLOYEENUMBER, LOCATION, HOURS_EXPOSED, FORMALDEHYDE_LEVEL
FROM         CSSI_FORMALDEHYDE_EXPOSURE
WHERE    CSSI_FORMALDEHYDE_EXPOSURE.LOCATION = CONVERT(nvarchar, ' + @location + ') AND CSSI_FORMALDEHYDE_EXPOSURE.SCANDATE &gt;= ' + '''' +  convert(varchar(50), @startDate) + '''' + ' AND CSSI_FORMALDEHYDE_EXPOSURE.SCANDATE &lt;= dateadd(hh, 24, ''' + convert(varchar(50), @endDate) +''')'


EXEC(@SQL)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@location">
            <Value>=Parameters!location.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="locations">
      <Fields>
        <Field Name="LOCATION">
          <DataField>LOCATION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="JOBLOCATION">
          <DataField>JOBLOCATION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="JOBDESCRIPTION">
          <DataField>JOBDESCRIPTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FORMALDEHYDE_LEVEL">
          <DataField>FORMALDEHYDE_LEVEL</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="LABEL">
          <DataField>LABEL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PK1</DataSourceName>
        <CommandText>SELECT DISTINCT 
                      'All Locations' AS LOCATION, NULL AS JOBLOCATION, NULL AS JOBDESCRIPTION, NULL AS FORMALDEHYDE_LEVEL, 'All Locations' AS LABEL
FROM         CSSI_FORMALDEHYDE_LEVELS
UNION ALL
SELECT     CONVERT(varchar, LOCATION) AS Expr1, JOBLOCATION, JOBDESCRIPTION, FORMALDEHYDE_LEVEL, CONVERT(varchar(4), LOCATION) 
                      + ' - ' + JOBDESCRIPTION AS LABEL
FROM         CSSI_FORMALDEHYDE_LEVELS AS CSSI_FORMALDEHYDE_LEVELS_1</CommandText>
      </Query>
    </DataSet>
  </DataSets>
