      <Query>
        <DataSourceName>PK1</DataSourceName>
        <CommandText>SELECT        CSSI_FORMALDEHYDE_DEFAULT.EMPLOYEENUMBER, CSSI_FORMALDEHYDE_DEFAULT.LOCATION, CSSI_FORMALDEHYDE_DEFAULT.DEPARTMENT, 
                         CSSI_FORMALDEHYDE_LEVELS.JOBLOCATION, CSSI_FORMALDEHYDE_LEVELS.JOBDESCRIPTION, CSSI_FORMALDEHYDE_TRAINING.TRAINED
FROM            CSSI_FORMALDEHYDE_DEFAULT INNER JOIN
                         CSSI_FORMALDEHYDE_TRAINING ON 
                         CSSI_FORMALDEHYDE_DEFAULT.EMPLOYEENUMBER = CSSI_FORMALDEHYDE_TRAINING.EMPLOYEENUMBER INNER JOIN
                         CSSI_FORMALDEHYDE_LEVELS ON CSSI_FORMALDEHYDE_DEFAULT.LOCATION = CSSI_FORMALDEHYDE_LEVELS.LOCATION</CommandText>
      </Query>
    </DataSet>
