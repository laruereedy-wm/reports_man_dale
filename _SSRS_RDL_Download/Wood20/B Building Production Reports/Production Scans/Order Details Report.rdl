        </Field>
        <Field Name="BLUETRACKING">
          <DataField>BLUETRACKING</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MONUMBER">
          <DataField>MONUMBER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MOTRACKING">
          <DataField>MOTRACKING</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BLUECARD">
          <DataField>BLUECARD</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="NUMBER">
          <Value>=iif(Fields!BLUEMONUMBER.Value is Nothing, Fields!MONUMBER.Value, Fields!BLUEMONUMBER.Value)</Value>
        </Field>
        <Field Name="TRACKING">
          <Value>=iif(Fields!BLUEMONUMBER.Value is Nothing, Fields!MOTRACKING.Value, Fields!BLUETRACKING.Value)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Expeditor</DataSourceName>
        <CommandText>SELECT     LASTSCAN.ORDRNO, LASTSCAN.ITEM, LASTSCAN.SCANTIME, BLUTRK.MONUMB AS BLUEMONUMBER, 
                      BLUTRK.TRACKING + BLUTRK.SUBTRACKING AS BLUETRACKING, OPENMOS.MANUFORDER AS MONUMBER, LASTSCAN.TRACKING AS MOTRACKING,
                       OPENMOS.BLUECARD
FROM         LASTSCAN LEFT OUTER JOIN
                      OPENMOS ON LASTSCAN.ORDRNO = OPENMOS.ORDERNO AND LASTSCAN.ITEM = OPENMOS.ITEMNO LEFT OUTER JOIN
                      BLUTRK ON LASTSCAN.ORDRNO = BLUTRK.ORDERNO AND LASTSCAN.ITEM = BLUTRK.ITEMNO
WHERE     (LASTSCAN.ORDRNO = @ordernumber) AND (LASTSCAN.ITEM &lt;&gt; '777') AND (LASTSCAN.ITEM &lt;&gt; '999')
ORDER BY LASTSCAN.ITEM</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ordernumber">
            <Value>=Parameters!ordernumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
  </DataSets>
