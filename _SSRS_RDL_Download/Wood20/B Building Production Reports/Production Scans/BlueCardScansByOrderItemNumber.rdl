        <Field Name="SCANTIME">
          <DataField>SCANTIME</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SCANGROUPING">
          <Value>=Fields!MONUMB.Value + Fields!SCANTIME.Value + Fields!Tracking.Value</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Expeditor</DataSourceName>
        <CommandText>SELECT     MONUMB, ORDERNO, ITEMNO, WANDDATE, TIME, TRACKING, SUBTRACKING, SCANTIME
FROM         vBLUEAUDT
WHERE     (ORDERNO = @ordernumber) AND (ITEMNO = CONVERT(nvarchar, @itemnumber))
UNION ALL
SELECT     MONUMB, ORDERNO, ITEMNO, WANDDATE, TIME, TRACKING, SUBTRACKING, SCANTIME
FROM         vBLUTRK
WHERE     (ORDERNO = @ordernumber) AND (ITEMNO = CONVERT(nvarchar, @itemnumber))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ordernumber">
            <Value>=Parameters!ordernumber.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@itemnumber">
            <Value>=Parameters!itemnumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
