    <DataSet Name="Maindata">
      <Fields>
        <Field Name="MOS_IMKEY">
          <DataField>MOS_IMKEY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MOS_WANTDATE">
          <DataField>MOS_WANTDATE</DataField>
