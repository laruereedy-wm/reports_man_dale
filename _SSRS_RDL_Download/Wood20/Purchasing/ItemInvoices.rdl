      </Fields>
      <Query>
        <DataSourceName>PK1</DataSourceName>
        <CommandText>SELECT     IM.IM_GL_ACCT, VL.VL_IMKEY, VL.VL_QTY, VL.VL_PRICE, VL.VL_SEQNUM, VL.VL_INV_NO, IM.IM_DESCR, VL.VL_DATE, IM.IM_ON_HAND, 
                      IM.IM_COST, IM.IM_STD_MAT
FROM         VL INNER JOIN
                      IM ON VL.VL_IMKEY = IM.IM_KEY
WHERE     (VL.VL_IMKEY = @partNumber)
ORDER BY VL.VL_IMKEY, VL.VL_DATE DESC, VL.VL_SEQNUM DESC, VL.VL_QTY</CommandText>
        <QueryParameters>
          <QueryParameter Name="@partNumber">
            <Value>=Parameters!partNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
