  <DataSets>
    <DataSet Name="mainData">
      <Query>
        <CommandText>SELECT     VL.VL_IMKEY, VL.VL_DATE, VL.VL_PHKEY, VL.VL_INV_NO, VL.VL_VNKEY, VN.VN_NAME, VL.VL_QTY, VL.VL_PRICE, VL.VL_QTY * VL.VL_PRICE AS Total,
                       IM.IM_DESCR
FROM         VL INNER JOIN
                      VN ON VL.VL_VNKEY = VN.VN_KEY INNER JOIN
                      IM ON VL.VL_IMKEY = IM.IM_KEY
WHERE     (VL.VL_DATE &lt;= @endDate) AND (VL.VL_IMKEY = @partNumber)
ORDER BY VL.VL_DATE DESC</CommandText>
        <QueryParameters>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@partNumber">
            <Value>=Parameters!partNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>PKI</DataSourceName>
      </Query>
      <Fields>
        <Field Name="VL_IMKEY">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>VL_IMKEY</DataField>
        </Field>
        <Field Name="VL_DATE">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>VL_DATE</DataField>
        </Field>
        <Field Name="VL_PHKEY">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>VL_PHKEY</DataField>
        </Field>
        <Field Name="VL_INV_NO">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>VL_INV_NO</DataField>
        </Field>
        <Field Name="VL_VNKEY">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>VL_VNKEY</DataField>
        </Field>
        <Field Name="VN_NAME">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>VN_NAME</DataField>
        </Field>
        <Field Name="VL_QTY">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>VL_QTY</DataField>
        </Field>
        <Field Name="VL_PRICE">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>VL_PRICE</DataField>
        </Field>
        <Field Name="Total">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Total</DataField>
        </Field>
        <Field Name="IM_DESCR">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>IM_DESCR</DataField>
        </Field>
        <Field Name="TEST">
          <Value>=Fields!VL_QTY.Value + Parameters!quantity.Value</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
