      <Query>
        <DataSourceName>PKI</DataSourceName>
        <CommandText>SELECT     IM.IM_KEY, IM.IM_DESCR, IM.IM_PROD_CODE, IM.IM_COMMODITY, IM.IM_TYPE, IM.IM_AVG_MAT, TX.TX_QTY
FROM         IM INNER JOIN
                      TX ON IM.IM_KEY = TX.TX_IMKEY
WHERE     (IM.IM_TYPE = 2) AND (IM.IM_PROD_CODE = 'BIS' OR
                      IM.IM_PROD_CODE = 'MTU' OR
                      IM.IM_PROD_CODE = 'FSK' OR
                      IM.IM_PROD_CODE = 'AER') AND (TX.TX_TYPE = 'A' OR
                      TX.TX_TYPE = 'I') AND (TX.TX_ORIGIN_STATUS = 'A') AND (TX.TX_DEST_STATUS = 'I') AND (TX.TX_DATE &gt;= @startDate) AND 
                      (TX.TX_DATE &lt;= @endDate) AND (IM.IM_COMMODITY = 'R21')
ORDER BY IM.IM_KEY</CommandText>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
