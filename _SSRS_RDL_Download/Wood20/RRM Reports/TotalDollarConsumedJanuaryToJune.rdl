      <Query>
        <DataSourceName>PKI</DataSourceName>
        <CommandText>SELECT     IM_KEY, IM_DESCR, IM_PROD_CODE, IM_COMMODITY, IM_TYPE, IM_AVG_MAT
FROM         IM
WHERE     (IM_TYPE = 2) AND (IM_PROD_CODE = 'AER' OR
                      IM_PROD_CODE = 'BIS' OR
                      IM_PROD_CODE = 'CAN' OR
                      IM_PROD_CODE = 'FSK' OR
                      IM_PROD_CODE = 'GLU' OR
                      IM_PROD_CODE = 'HMT' OR
                      IM_PROD_CODE = 'LAM' OR
                      IM_PROD_CODE = 'MTU' OR
                      IM_PROD_CODE = 'PUT' OR
                      IM_PROD_CODE = 'RAG' OR
                      IM_PROD_CODE = 'REM' OR
                      IM_PROD_CODE = 'REW' OR
                      IM_PROD_CODE = 'SCO' OR
                      IM_PROD_CODE = 'SEA' OR
                      IM_PROD_CODE = 'STA' OR
                      IM_PROD_CODE = 'STW' OR
                      IM_PROD_CODE = 'TAC' OR
                      IM_PROD_CODE = 'THI' OR
                      IM_PROD_CODE = 'VAR' OR
                      IM_PROD_CODE = 'WAX' OR
                      IM_PROD_CODE = 'HIC' OR
                      IM_PROD_CODE = 'HIN' OR
                      IM_PROD_CODE = 'KNO' OR
                      IM_PROD_CODE = 'PUL') AND (IM_COMMODITY = 'R21' OR
                      IM_COMMODITY = 'R01' OR
                      IM_COMMODITY = 'R10' OR
                      IM_COMMODITY = 'R02' OR
                      IM_COMMODITY = 'M01' OR
                      IM_COMMODITY = 'R04' OR
                      IM_COMMODITY = 'R20')
ORDER BY IM_KEY</CommandText>
