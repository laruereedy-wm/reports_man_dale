  <DataSources>
    <DataSource Name="PKI">
      <DataSourceReference>PKI</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>7633128f-7e73-4b68-bfd2-b239073072aa</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="mainData">
      <Query>
        <DataSourceName>PKI</DataSourceName>
        <CommandText>SELECT IM.IM_KEY, IM.IM_DESCR, IM.IM_ON_HAND, IM.IM_ALLOC, IM.IM_SAFETY, IM.IM_STOCK_SW, IM.IM_TYPE, ML.ML_QTY, ML.ML_LOCATION

FROM PK1.dbo.ML ML INNER JOIN PK1.dbo.IM IM ON ML.ML_IMKEY=IM.IM_KEY

WHERE (IM.IM_STOCK_SW=1 AND IM.IM_TYPE=2 AND IM.IM_ON_HAND&lt;IM.IM_ALLOC AND ML.ML_LOCATION='HP' OR IM.IM_STOCK_SW=1 AND IM.IM_TYPE=2 AND IM.IM_ON_HAND&lt;IM.IM_ALLOC AND ML.ML_LOCATION LIKE 'SR%')

ORDER BY IM.IM_KEY</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IM_KEY">
          <DataField>IM_KEY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IM_DESCR">
